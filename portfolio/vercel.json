{
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    { "source": "/", "destination": "/.dist/index.html" },
    { "source": "/blog/", "destination": "/.dist/blog/index.html" },
    { "source": "/blog/:slug", "destination": "/.dist/blog/:slug.html" },
    { "source": "/(.*)", "destination": "/.dist/$1" }
  ]
}
